<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>com.github.cuioss</groupId>
        <artifactId>cuioss-parent-java</artifactId>
        <version>1.0-SNAPSHOT</version>
        <relativePath />
    </parent>
    <artifactId>cuioss-java-utils</artifactId>
    <name>cuioss-java-utils</name>
    <description>
        A number of additional utilites on top on guava-light
    </description>
    <version>27.1-SNAPSHOT</version>
    <url>https://github.com/cuioss/cuioss-java-utils/</url>
    <scm>
        <url>https://github.com/cuioss/cuioss-java-utils/</url>
        <connection>https://github.com/cuioss/cuioss-java-utils.git</connection>
        <developerConnection>scm:git:https://github.com/cuioss/cuioss-java-utils/</developerConnection>
    </scm>
    <issueManagement>
        <url>https://github.com/cuioss/cuioss-java-utils/issues</url>
        <system>GitHub Issues</system>
    </issueManagement>
    <properties>
        <cuiosss.dependencies.version>1.0-SNAPSHOT</cuiosss.dependencies.version>
    </properties>
    <dependencyManagement>
        <dependencies>
            <dependency>
                <groupId>com.github.cuioss</groupId>
                <artifactId>cuioss-dependency-management</artifactId>
                <version>${cuiosss.dependencies.version}</version>
                <type>pom</type>
                <scope>import</scope>
            </dependency>
        </dependencies>
    </dependencyManagement>
    <dependencies>
        <dependency>
            <groupId>com.github.cuioss</groupId>
            <artifactId>guava-light</artifactId>
        </dependency>
        <dependency>
            <groupId>org.projectlombok</groupId>
            <artifactId>lombok</artifactId>
        </dependency>
        <!-- Unit testing -->
        <dependency>
            <groupId>org.junit.jupiter</groupId>
            <artifactId>junit-jupiter</artifactId>
        </dependency>
        <dependency>
            <groupId>org.hamcrest</groupId>
            <artifactId>hamcrest-core</artifactId>
        </dependency>
    </dependencies>
</project>